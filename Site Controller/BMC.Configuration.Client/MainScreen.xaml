        <DataTemplate x:Uid="DataTemplate_1" x:Key="NameTemplate">
            <Grid x:Uid="Grid_1" MinWidth="10">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="Name" Text="{Binding Name}" x:Name="Name" TextWrapping="Wrap" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Uid="DataTemplate_2" x:Key="ValueTemplate">
            <Grid x:Uid="Grid_2" MinWidth="10">
                <Border x:Uid="Border_2" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="Value" Text="{Binding Value}" x:Name="Value" TextWrapping="Wrap" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="ColorStatus"  x:Uid="DataTemplate_0">
            <Grid x:Uid="Grid_1"  MinWidth="10" HorizontalAlignment="Stretch">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-10,-2,-5.5,-2" BorderThickness="0,0,0.5,0" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="UidServiceStatus" x:Name="ServiceStatus" Text="{Binding Path=ServiceStatus}" HorizontalAlignment="Center" Margin="0,0,0,0" />
            </Grid>
            <DataTemplate.Triggers>
                <DataTrigger x:Uid="DataTrigger_1" Binding="{Binding ServiceStatus}" Value="Stopped">
                    <Setter x:Uid="Setter_1" TargetName="ServiceStatus" Property="Foreground" Value="Red" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_2" Binding="{Binding ServiceStatus}" Value="Running">
                    <Setter x:Uid="Setter_2" TargetName="ServiceStatus" Property="Foreground" Value="Blue" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_3" Binding="{Binding ServiceStatus}" Value="Pending">
                    <Setter x:Uid="Setter_3" TargetName="ServiceStatus" Property="Foreground" Value="MediumSeaGreen" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_4" Binding="{Binding ServiceStatus}" Value="Service not found">
                    <Setter x:Uid="Setter_4" TargetName="ServiceStatus" Property="Foreground" Value="Green" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_5" Binding="{Binding ServiceStatus}" Value="Start Failed">
                    <Setter x:Uid="Setter_5" TargetName="ServiceStatus" Property="Foreground" Value="Red" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_6" Binding="{Binding ServiceStatus}" Value="Pending Stopped">
                    <Setter x:Uid="Setter_6" TargetName="ServiceStatus" Property="Foreground" Value="MediumSeaGreen" />
                </DataTrigger>
            </DataTemplate.Triggers>
        </DataTemplate>
        <DataTemplate x:Key="ColorServiceName"  x:Uid="DataTemplate_0">
            <Grid x:Uid="Grid_1"  MinWidth="10" HorizontalAlignment="Stretch">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0,0,0.5,0" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="UidServiceStatus" x:Name="ServiceNameStatus" Text="{Binding Path=ServiceName}" HorizontalAlignment="Center" Margin="0,0,0,0" />
            </Grid>
            <DataTemplate.Triggers>
                <DataTrigger x:Uid="DataTrigger_1" Binding="{Binding ServiceStatus}" Value="Stopped">
                    <Setter x:Uid="Setter_1" TargetName="ServiceNameStatus" Property="Foreground" Value="Red" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_2" Binding="{Binding ServiceStatus}" Value="Running">
                    <Setter x:Uid="Setter_2" TargetName="ServiceNameStatus" Property="Foreground" Value="Blue" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_3" Binding="{Binding ServiceStatus}" Value="Pending">
                    <Setter x:Uid="Setter_3" TargetName="ServiceNameStatus" Property="Foreground" Value="MediumSeaGreen" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_4" Binding="{Binding ServiceStatus}" Value="Service not found">
                    <Setter x:Uid="Setter_4" TargetName="ServiceNameStatus" Property="Foreground" Value="Green" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_5" Binding="{Binding ServiceStatus}" Value="Start Failed">
                    <Setter x:Uid="Setter_5" TargetName="ServiceNameStatus" Property="Foreground" Value="Red" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_6" Binding="{Binding ServiceStatus}" Value="Pending Stopped">
                    <Setter x:Uid="Setter_6" TargetName="ServiceNameStatus" Property="Foreground" Value="MediumSeaGreen" />
                </DataTrigger>
            </DataTemplate.Triggers>
        </DataTemplate>
        <DataTemplate x:Uid="DataTemplate_1" x:Key="checkbox">
            <CheckBox x:Uid="CheckBox_1" IsEnabled="True" IsChecked="{Binding Check}" Margin="0,1,1,1" />
        </DataTemplate>
