        <ExchangeConfiguration:ExchangeConfigurationEntity x:Key="ExchangeConfigurationSource">
        </ExchangeConfiguration:ExchangeConfigurationEntity>
        <ControlTemplate
            x:Key="validationTemplate">
            <DockPanel Focusable="False">
                <TextBlock Focusable="False" Foreground="Red"
