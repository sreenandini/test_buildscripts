<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="LogPath" value="C:\Logs\CashDeskOperator.txt"/>
    <add key="LogMode" value="TXT"/>
    <add key="LogLevel" value="1"/>
    <add key="ApplicationName" value="Cash Desk Operator"/>
    <add key="RefreshTime" value="5"/>
    <add key="VoidRecords" value="20"/>
    <add key="AllowDragging" value="true"/>
    <add key="CardNumberFormat" value=";([A-Z,a-z,0-9])*\?"/>
    <add key="InnerCardNumberFormat" value="([A-Z,a-z,0-9])*"/>
    <add key="NumberOfCharToTrim" value="4"/>
    <add key="NoOfRecords" value="15"/>
    <add key="ReedeemScreenDisplayTime" value="60"/>
    <add key="HandpayCommandLinePrompt" value="cmdline.bat"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
    <add key="NetLoggerPath" value="NetLogger"/>
    <add key="RegistryPath" value="Cashmaster"/>
    <add key="ExchangeClientInstallationPath" value=""/>
    <add key="ServicesList" value="BGSExchangeHost,BGSExchangeMonitorMSMQ,BGS Automated Read,BGS Hourly Read,BMC Exchange Data Import Export,BMCGuardian"/>
    <add key="ServicesListFromDB" value="TRUE"/>
    <add key="ServiceRecords" value="10"/>
    <add key="Server" value="Enterprise"/>
    <add key="ServerIPCUrl" value=":9092/Server.Rem"/>
    <add key="SiteLicensingServerIPCUrl" value=":9095/SiteLicensingServer.Rem"/>
    <!--Value takes the No. of seconds to check the remote server connectivity-->
    <add key="RemoteServerConnectionCheck" value="600"/>
    <!--Value takes the No. of seconds same as in Data Import Export Service Config-->
    <add key="IPCInterval" value="5"/>

    <add key="XDays" value="5"/>
    <add key="StartPathCDM" value="C:\Program Files\Bally Technologies\BMC Cash Desk Manager\"/>
    <add key="ZipPath" value="C:\Program Files\Bally Technologies\Scripts\"/>
    <add key="ExchangeKey" value="Software\\Honeyframe\\Cashmaster\\ExchangeKey"/>
    <add key="EnterpriseKey" value="Software\\Honeyframe\\Cashmaster\\EnterpriseKey"/>
    <add key="WebserviceExtension" value="/EnterpriseWebservice/EnterpriseWebservice.asmx"/>
    <add key="TicketExpire" value="Software\\Honeyframe\\Cashmaster\\Exchange\\TktExpDays"/>
    <add key="GetDefaultCultureForRegion" value="en-GB"/>
    <add key="GetDefaultCultureForCurrency" value="en-GB"/>
    <add key="GetDefaultCultureForDate" value="en-GB"/>
    <add key="GetDefaultCultureForUserLanguage" value="en-GB"/>
    <add key="InstallType" value="SOFTWARE\\Honeyframe"/>
    <add key="EnableDenomChange" value="FALSE"/>
    <add key="isDebug" value="FALSE"/>
    <add key="CoinDispenserCOMPort" value="COM1"/>
    <!--Site Interrogation 10000-->
    <add key="DBRefreshInterval" value="3000"/>
    <add key="NoOfDropMachinesPerThread" value="20"/>
    <add key="PerformDropWaitTime" value="1"/>
    <add key="NoOfDeclarationMachinesPerThread" value="20"/>
    <add key="DeclarationWaitTime" value="1"/>
    <add key="DisplayEventsCount" value="20"/>
    <!--Cash Entry-->
    <add key="it-IT" value="FIVES,TENS,TWENTIES,FIFTIES,HUNDREDS,TWO_HUNDREDS,FIVE_HUNDREDS"/>
    <add key="es-ar" value="TWOS,FIVES,TENS,TWENTIES,FIFTIES,HUNDREDS,TWO_HUNDREDS,FIVE_HUNDREDS"/>
    <add key="en-GB" value="FIVES,TENS,TWENTIES,FIFTIES"/>
    <add key="en-US" value="ONES,FIVES,TENS,TWENTIES,FIFTIES,HUNDREDS"/>

    <add key="en-US_Denom" value="1,5,10,20,50,100:0.01,0.05,0.10,0.25,0.50 "/>
    <add key="en-GB_Denom" value="5,10,20,50:0.02,0.05,0.1,0.2,0.5"/>
    <add key="ShowUncommitedChangesPopup" value="True"/>
    <add key="ShowErrorAsPopup" value="True"/>
    <add key="AutoSaveOnMoveNext" value="False"/>
    <add key="ExcludeVouchersNotInSystem" value="False"/>
    <add key="VoucherAutoAddOnLength" value="18"/>
    <add key="DisableVoucherAutoAddOnLength" value="False"/>
    <add key="RefreshVouchersOnSave" value="False"/>
    <!--END Cash Entry-->

    <!--Update GMU Bin-->
    <add key="GMUBinLocation" value="b908305j.bin"/>
    <add key="TFTPServerIP" value="192.168.2.6"/>
    <add key="GMUBINAPPLICATION" value="C:\usr\bin\"/>
    <add key="GMURETRY" value="0"/>
    <add key="GMUTIMEOUT" value="3"/>
    <add key="GMUoptionfilelocation" value="b908305j.opt"/>
    <add key="TFTPserverpath" value="C:\GMU\Tftpd32\tftpd32.exe"/>
    <!--Update GMU Binoption file-->

    <!--Ping GMU Start-->
    <add key="IsPingGMUEnabled" value="True"/>
    <add key="PingTimeout" value="1000"/>
    <!--Stop Ping GMU-->

    <!--Glory CD-->
    <add key="CD_Duplex" value="TRUE"/>
    <add key="CallBackTimeOutValue"  value="30"/> <!--In Sec-->

    <add key="Player_RatingMessages" value="All,PT77(Card-In),PT66(Final Interval Rating),PT65(Interval Rating),PT10(Jackpot Rating)"/>
    <add key="IsPlayerRatingEnabled" value="1"/>
    <add key="SendSingleDropAlert" value="True"/>
    <add key="NoOfMaxPositionsPerAlert" value="250"/>
    <!--Player Data resend interval(milliseconds)-->
    <add key="PDResendInterval" value="50"/>
    <add key="ReadForLiquidationRecords" value="50" />
    <!--GMUNoUpdate-->
    <add key="GMUNoUpdateRetryCount" value="3"/>
    <add key="ThemeRequiered" value="true"/>

    <!-- PRINTER DELAY -->
    <add key="PrinterDelayTime" value="500"/>
    <add key="PrinterDelayloopCount" value="10"/>

    <!-- TIS Screen-->
    <add key="DisplayTISRecords" value="15"/>
    <add key="DisplayPromoHistory" value="14"/>
    <add key="DisplayPromoTicketPrintMessage" value="False"/>

    <!-- CDO Central Server Settings -->
    <add key="CDOCentralServerPortNo" value="9099"/>
    <add key="LogIncomingMessages" value="False"/>
    <add key="LogOutgoingMessages" value="False"/>
    <add key="SetSlotStatusWaitTimeout" value="5000"/>
    <add key="SetFloorPositionWaitTimeout" value="5000"/>
    <add key="SetServiceStatusWaitTimeout" value="5000"/>
    <add key="LogClients" value="False"/>
    <!-- ITHACA Frimware Version Old(PP8522) New(PP8576) -->
    <add key="ITHACAFRIMWAREVERSION" value="PP8576"/>

    <!-- Redeem Multiple Tickets-->
    <add key ="MultipleTicketRedeemAutoClearGrid" value="10"/>
    <add key ="CDM_RecordsCount" value="1000"/>
    <!--Floor View Locations based on the user -->
    <add key ="FloorViewUserLocations" value="True"/>
    <!--timer in seconds-->
    <add key ="PlayerClubScreenRefreshTime" value="10"/>


   <!-- Minimize button in Main Screen-->
    <add key ="IsMinimizeButtonEnabled" value="True"/>
  
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_IBally"  closeTimeout="00:01:00"
            openTimeout="00:01:00" receiveTimeout="00:01:00" sendTimeout="00:00:30"
            transactionFlow="false" transferMode="Buffered" transactionProtocol="OleTransactions"
            hostNameComparisonMode="StrongWildcard" listenBacklog="10"
            maxBufferPoolSize="524288" maxBufferSize="65536" maxConnections="10"
            maxReceivedMessageSize="65536">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384"
              maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <reliableSession ordered="true" inactivityTimeout="00:03:00"
              enabled="false"  />
          <security mode="None">
            <transport clientCredentialType="Windows" protectionLevel="EncryptAndSign"/>
            <message clientCredentialType="Windows"/>
          </security>
        </binding>
      </netTcpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="CallbackBehavior">
          <callbackTimeouts transactionTimeout="00:00:10"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>

    </client>
  </system.serviceModel>
<startup useLegacyV2RuntimeActivationPolicy="true"><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
