<Window x:Class="BMC.Presentation.POS.Views.CashEntry"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="CashEntry"
    WindowStartupLocation="CenterScreen" ShowInTaskbar="False" 
    Height="768" Width="1024"  
    AllowsTransparency="True" 
    WindowStyle="None" Background="{DynamicResource MainScreenBG}"
    xmlns:Helper_classes="clr-namespace:BMC.Presentation.POS.Helper_classes"
    xmlns:CashDeskOperator="clr-namespace:BMC.CashDeskOperator;assembly=BMC.CashDeskOperator"  Loaded="Window_Loaded"
    >
    <Window.Resources>
        <Helper_classes:DecimalValueConverter x:Uid="source:DecimalValueConverter" x:Key="DecimalValueConverter" />
        <Helper_classes:WeekBatchGridVisibility x:Uid="source:WeekBatchGridVisibility" x:Key="WeekBatchGridVisibility" />
        <Helper_classes:ReasonConverter x:Uid="source:ReasonConverter" x:Key="ReasonConverter" />
        <DataTemplate x:Key="DTBill">
            <Grid x:Uid="Grid_1" MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2"  BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock  Text="{Binding Bill}" HorizontalAlignment="Right" Margin="0,0,0,0" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DTValue">
            <Grid x:Uid="Grid_1" MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBox x:Uid="TextBlock_1" Text="{Binding Value}" HorizontalAlignment="Right" Width="75" Margin="0,0,0,0" />
            </Grid>
        </DataTemplate>
      
        <DataTemplate x:Key="DTDelete">
            <Grid x:Uid="Grid_3">
                <Border x:Uid="Border_2" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch"  VerticalAlignment="Stretch" />
                <Button FontSize="14" FontWeight="Bold" FontFamily="Segoe UI"  Click="btn_DeleteVoucher_Click"  Width="30" Height="30" >
                    <Button.Background>
                        <ImageBrush ImageSource="..\Images\delete.png"></ImageBrush>
                    </Button.Background>
                </Button>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DTCanUpdate">
            <Grid x:Uid="Grid_1" MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
               <CheckBox x:Uid="chkIsSelected"  IsChecked="{Binding IsSelected, Mode=TwoWay}" Visibility="{Binding CanUpdate,Mode=OneWay, Converter={StaticResource WeekBatchGridVisibility}}" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DTVoucher">
            <Grid x:Uid="Grid_1" MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock  Text="{Binding strBarcode}" Margin="0,0,0,0"/>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DTStatus">
            <Grid MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock  Text="{Binding Status}"  Margin="0,0,0,0"/>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DTAmount">
            <Grid x:Uid="Grid_1" MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="TextBlock_1" TextAlignment="Right" Text="{Binding iAmount,Converter={StaticResource DecimalValueConverter}}" Margin="0,0,0,0" />
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DTReason">
            <Grid x:Uid="Grid_1" MinWidth="0">
                <Border x:Uid="Border_1" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0.5,0,0.5,0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="TextBlock_1" Text="{Binding Reasonid,Converter={StaticResource ReasonConverter}}" Margin="0,0,0,0" />
            </Grid>
        </DataTemplate>
        <Style TargetType="{x:Type TextBlock}" x:Key="myTextBlock">
            <Setter Property="Margin" Value="5,0,0,0" />
            <Setter Property="Width" Value="30" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Foreground"  Value="Red" />
        </Style>
    </Window.Resources>

    <Grid VerticalAlignment="Top"  Name="grdMain" Height="750" Width="1001">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" MinHeight="20"></RowDefinition>
            <RowDefinition Height="71" MinHeight="50"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="150"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <!-- HEADER-->
        <Grid  Grid.Row="0" Margin="0,0,0,0" Name="grdHeader">
            <Grid.RowDefinitions>
                <RowDefinition ></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBlock x:Uid="txtHeader" x:Name="txtHeader" Margin="9,5,11,16.727" TextAlignment="Left" FontFamily="Segoe UI" FontWeight="Bold" FontSize="18" RenderTransformOrigin="0.5,0.5" Text="{DynamicResource BillsTicketCounter_xaml_txtHeader}" Foreground="#FFFFFFFF" />
            <Path x:Uid="Path_1" Stretch="Fill" Data="F1 M 1065.21,1764.57L 1263.19,1764.57C 1263.74,1764.57 1264.19,1765.02 1264.19,1765.57L 1264.19,1765.57C 1264.19,1766.12 1263.74,1766.57 1263.19,1766.57L 1065.21,1766.57C 1064.66,1766.57 1064.21,1766.12 1064.21,1765.57L 1064.21,1765.57C 1064.21,1765.02 1064.66,1764.57 1065.21,1764.57 Z " HorizontalAlignment="Left" VerticalAlignment="Top" Width="199.975" Height="1.998" Margin="20,40,0,0">
                <Path.Fill>
                    <LinearGradientBrush x:Uid="LinearGradientBrush_1" StartPoint="-1.22086e-006,0.499939" EndPoint="0.999997,0.499939">
                        <GradientStop x:Uid="GradientStop_1" Color="#FFC4CFBE" Offset="0" />
                        <GradientStop x:Uid="GradientStop_2" Color="#00C4CFBE" Offset="1" />
                    </LinearGradientBrush>
                </Path.Fill>
            </Path>
            <TextBlock x:Uid="txtSiteInfo" Grid.Row="0" TextAlignment="Center" Grid.Column="1"  x:Name="txtSiteInfo" Margin="9,5,11,16.727"  FontFamily="Segoe UI" FontWeight="Bold" FontSize="18" RenderTransformOrigin="0.5,0.5" Text="{DynamicResource BillsTicketCounter_xaml_txtHeader}" Foreground="#FFFFFFFF" />
           
            <Button Grid.Row="0" Grid.Column="2" HorizontalAlignment="Right" x:Uid="btnExit" Width="54.728" Height="58.725" Content="{DynamicResource Login_xaml_btnExit}" Style="{DynamicResource BMC_App_Exit}" x:Name="btnExit" VerticalAlignment="Top"  Margin="0,5,5,0" Click="btnExit_Click"  />
        </Grid>
        <!--Asset Details-->
        <Grid Grid.Row="1"  Background="{DynamicResource LoginInputBG}"  Margin="0,0,0,0" Name="grdAssetDetails" Height="65" VerticalAlignment="Center" HorizontalAlignment="Stretch">
            <Grid.RowDefinitions>
                <RowDefinition ></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                 <ColumnDefinition Width="*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <TextBlock x:Name="lblPosition"  Grid.Column="0"  Text="{DynamicResource BillsTicketCounter_xaml_lblPosition}" FontFamily="Segoe UI" FontSize="16" FontWeight="Bold"  HorizontalAlignment="Stretch"  ClipToBounds="False" TextAlignment="Center" MinWidth="50" MaxHeight="50" VerticalAlignment="Center"></TextBlock>
            <TextBlock x:Name="lblAsset"  Grid.Column="2" Text="{DynamicResource BillsTicketCounter_xaml_lblAsset}" FontFamily="Segoe UI" FontSize="16" FontWeight="Bold"  HorizontalAlignment="Stretch" ClipToBounds="False" TextAlignment="Center" MinWidth="50" MaxHeight="50" VerticalAlignment="Center"></TextBlock>
            <TextBlock x:Name="lblGame"  Grid.Column="4"  Text="{DynamicResource BillsTicketCounter_xaml_lblGame}" FontFamily="Segoe UI" FontSize="16" FontWeight="Bold"  HorizontalAlignment="Stretch"  ClipToBounds="False" TextAlignment="Center" MinWidth="50" MaxHeight="50" VerticalAlignment="Center"></TextBlock>
            <!--<TextBox Grid.Column="1"  x:Uid="txtPosition" VerticalContentAlignment="Center"  TextWrapping="NoWrap" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}" Height="25.6" Foreground="{DynamicResource TextBoxForeground}" FontSize="16" x:Name="txtPosition" Style="{DynamicResource TextBoxStyle1}" FontFamily="Segoe UI" IsReadOnly="True" Margin="18,6.1,13,12.2" />-->
         
            <!--<ComboBox FontSize="16" x:Uid="cmbPosition"  Grid.Column="1" Style="{DynamicResource BMC_ComboBoxStyle}"  DisplayMemberPath="Position" 
                          x:Name="cmbPosition" SelectionChanged="cmbPosition_SelectionChanged" Margin="0,19.843,0,18.86"></ComboBox>-->

            <ComboBox x:Uid="cmbPosition" PreviewMouseDown="cmbPosition_PreviewMouseDown" Grid.Column="1" Margin="0,0,0,0" Height="30" IsSynchronizedWithCurrentItem="True" Style="{DynamicResource BMC_ComboBoxStyle}"  DisplayMemberPath="Position" 
                          x:Name="cmbPosition" KeyboardNavigation.TabIndex="0" PreviewKeyDown="EnterClicked">
            </ComboBox>

            <TextBox FontSize="18" Text="{Binding Path=AssetNo}"  Grid.Column="3" x:Uid="txtAsset" VerticalContentAlignment="Center"  TextWrapping="NoWrap" Background="{DynamicResource TextBoxDIsableGradient}" BorderBrush="{DynamicResource TextBoxOutline}" Foreground="{DynamicResource TextBoxForeground}" x:Name="txtAsset" Style="{DynamicResource TextBoxStyle1}" FontFamily="Segoe UI" IsReadOnly="True"  ClipToBounds="False"  VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Left" />
            <TextBox  FontSize="18" Text="{Binding Path=GameTitle}"   Grid.Column="5"  x:Uid="txtGame" TextWrapping="NoWrap" Background="{DynamicResource TextBoxDIsableGradient}" VerticalContentAlignment="Center" BorderBrush="{DynamicResource TextBoxOutline}" Foreground="{DynamicResource TextBoxForeground}" x:Name="txtGame" Style="{DynamicResource TextBoxStyle1}" FontFamily="Segoe UI" IsReadOnly="True" ClipToBounds="False" Grid.ColumnSpan="2" Margin="0,17.53,27,17.53" />
        </Grid>
        <!--Cash Entry-->
        <Grid Grid.Row="2" Name="grdCashEntry" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="280"></ColumnDefinition>
                <ColumnDefinition Width="303"></ColumnDefinition>
                <ColumnDefinition Width="428*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition  Height="Auto" MinHeight="400"></RowDefinition>
                <RowDefinition  Height="Auto" MinHeight="100"></RowDefinition>
            </Grid.RowDefinitions>
            <!--DATA LISTS-->
            <GroupBox x:Uid="GroupBox_1" Name="grp_bills" FontFamily="Segoe UI" FontSize="14" FontWeight="Normal" Height="400" VerticalAlignment="Top" Margin="0,0,8,0">
                <GroupBox.Header x:Uid="GroupBox_1_Header" >
                    <GroupItem FontFamily="Segoe UI" FontSize="16" FontWeight="Bold" Height="38.867" Width="Auto">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch"> 
                            <Label Margin="5,-3,0,0"  Content="{DynamicResource BillsTicketCounter_xaml_Bills_Counted}" Width="180"></Label>
                            <Button Height="26" Name="btn_ClearBils" Foreground="White" Background="Black"  Click="btn_ClearBils_Click" Content="{DynamicResource CashEntry_xaml_btnClear}" Width="60"></Button>
                        </StackPanel>
                    </GroupItem>
                </GroupBox.Header>
                    <Grid VerticalAlignment="Top" ShowGridLines="False" >
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="80*"></ColumnDefinition>
                            <ColumnDefinition Width="60*"></ColumnDefinition>
                            <ColumnDefinition Width="121*"></ColumnDefinition>
                            <!--<ColumnDefinition Width="10*"></ColumnDefinition>-->
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="41" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto"  ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                            <RowDefinition Height="Auto" ></RowDefinition>
                        </Grid.RowDefinitions>
  
                        <TextBlock  Grid.Row="0" Background="#182E3D" Height="30" Foreground="White" FontSize="14" TextAlignment="Center"   Visibility="Visible" Grid.Column="0"  Name="lblDenomheader"   Margin="0,0,0,0" Text="{DynamicResource CashEntry_lblDenomheader}" TextWrapping="Wrap"  VerticalAlignment="Bottom"  />
                        <TextBlock Grid.Column="1" Background="#182E3D" Height="30" Foreground="White"  FontSize="14" TextAlignment="Center" Visibility="Visible" Grid.Row="0" Name="lblCountHeader"   Margin="1,0,0,0" Text="{DynamicResource CashEntry_lblCountHeader}"  TextWrapping="Wrap"  VerticalAlignment="Bottom"  />
                        <TextBlock Grid.Column="2" Background="#182E3D" Height="30" Foreground="White"  FontSize="14" TextAlignment="Center" Visibility="Visible" Grid.Row="0" Name="lblAmountHeader"  Margin="1,0,0,0" Text="{DynamicResource CashEntry_lblAmountHeader}"  TextWrapping="Wrap"  VerticalAlignment="Bottom"  />
                  
                        <TextBlock  Grid.Row="9" Visibility="Collapsed" Grid.Column="0" Name="lbl_Ones"   Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap"  VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="8" Visibility="Collapsed" Grid.Column="0" Name="lbl_Twos"   Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap"  VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="7" Visibility="Collapsed" Grid.Column="0" Name="lbl_Fives"  Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap"  VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="6" Visibility="Collapsed" Grid.Column="0" Name="lbl_Tens"  Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap" VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="5" Visibility="Collapsed" Grid.Column="0" Name="lbl_Twenties"  Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap" VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="4" Visibility="Collapsed" Grid.Column="0" Name="lbl_Fifties"  Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap"  VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="3" Visibility="Collapsed" Grid.Column="0" Name="lbl_Hundreds"  Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap"  VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="2" Visibility="Collapsed" Grid.Column="0" Name="lbl_TwoHundreds"  Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap"  VerticalAlignment="Center"  />
                        <TextBlock Grid.Row="1" Visibility="Collapsed" Grid.Column="0" Name="lbl_FiveHundreds" Margin="5,0,0,0" Text="{DynamicResource CCashDeskManager_xaml_TextBlock_24}" TextWrapping="Wrap" VerticalAlignment="Center"  />
                        <!--<TextBlock Grid.Row="9" Visibility="Visible" Grid.Column="0" Name="lbl_BillTotal" Margin="5,0,0,0" Text="{DynamicResource BillsTicketCounter_xaml_lblTotal}" TextWrapping="Wrap" VerticalAlignment="Center"  />-->
                        <TextBlock Grid.Row="11" Visibility="Visible" Grid.Column="0" Name="lbl_CoinsTotal" Margin="5,0,0,0" Text="{DynamicResource CashEntry_xaml_lbl_TotalCoins}" TextWrapping="Wrap" VerticalAlignment="Center"  />
                        
                    <TextBox Grid.Row="9" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="9" Name="txt_Onescnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap"  Height="25" VerticalAlignment="Top" Width="50" />
                    <TextBox Grid.Row="8" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="8" Name="txt_Twoscnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50"/>
                    <TextBox Grid.Row="7" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="7" Name="txt_Fivescnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50" />
                    <TextBox Grid.Row="6" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="6" Name="txt_Tenscnt"  Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50"/>
                    <TextBox Grid.Row="5" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="5" Name="txt_Twentiescnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50" />
                    <TextBox Grid.Row="4" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="4" Name="txt_Fiftiescnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50" />
                    <TextBox Grid.Row="3" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="3" Name="txt_Hundredscnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50" />
                    <TextBox Grid.Row="2" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="2" Name="txt_TwoHundredscnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50"/>
                    <TextBox Grid.Row="1" Visibility="Collapsed"  PreviewMouseUp="txtNumber_PreviewMouseUp" PreviewTextInput="NumericOnly"  MaxLength="5" TextChanged="txt_TextChanged" Grid.Column="1" KeyboardNavigation.TabIndex="1" Name="txt_FiveHundredscnt" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="False" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="NoWrap" Height="25" VerticalAlignment="Top" Width="50"/>
                    <!--<TextBox Grid.Row="9"   Grid.Column="1" Name="txt_BillsTotal" Background="{DynamicResource TextBoxGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="True" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="Wrap" Height="25" VerticalAlignment="Top" Width="100" />-->

                    <TextBox  Grid.Row="9" Visibility="Collapsed" PreviewMouseUp="txAmount_PreviewMouseUp"  PreviewTextInput="NumericOnly" MaxLength="5" GotFocus="GetFocused" KeyUp="txt_Ones_KeyUp"  Text="{Binding Path=P100, Mode=OneWay }"  Grid.Column="2" Name="txt_Ones" Background="{DynamicResource TextBoxDIsableGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="True" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="Wrap" Height="25" VerticalAlignment="Top" Width="100" PreviewKeyDown="EnterClicked" KeyboardNavigation.TabIndex="9"/>
                    <TextBox Grid.Row="8"  Visibility="Collapsed" PreviewMouseUp="txAmount_PreviewMouseUp"  PreviewTextInput="NumericOnly" MaxLength="6" GotFocus="GetFocused" KeyUp="txt_Ones_KeyUp"  Text="{Binding Path=P200, Mode=OneWay}" Grid.Column="2" Name="txt_Twos" Background="{DynamicResource TextBoxDIsableGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="True" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="Wrap" Height="25" VerticalAlignment="Top" Width="100" PreviewKeyDown="EnterClicked" KeyboardNavigation.TabIndex="8"/>
                    <TextBox Grid.Row="7" Visibility="Collapsed" PreviewMouseUp="txAmount_PreviewMouseUp"  PreviewTextInput="NumericOnly" MaxLength="6"  GotFocus="GetFocused" KeyUp="txt_Ones_KeyUp"   Text="{Binding Path=P500, Mode=OneWay}" Grid.Column="2" Name="txt_Fives" Background="{DynamicResource TextBoxDIsableGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="True" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="Wrap" Height="25" VerticalAlignment="Top" Width="100" PreviewKeyDown="EnterClicked" KeyboardNavigation.TabIndex="7"/>
                    <TextBox Grid.Row="6" Visibility="Collapsed" PreviewMouseUp="txAmount_PreviewMouseUp"  PreviewTextInput="NumericOnly" MaxLength="6"  GotFocus="GetFocused" KeyUp="txt_Ones_KeyUp"  Text="{Binding Path=P1000, Mode=OneWay}" Grid.Column="2" Name="txt_Tens"  Background="{DynamicResource TextBoxDIsableGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="True" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="Wrap" Height="25" VerticalAlignment="Top" Width="100" PreviewKeyDown="EnterClicked" KeyboardNavigation.TabIndex="6"/>
                    <TextBox Grid.Row="5" Visibility="Collapsed" PreviewMouseUp="txAmount_PreviewMouseUp"  PreviewTextInput="NumericOnly" MaxLength="7"  GotFocus="GetFocused" KeyUp="txt_Ones_KeyUp"  Text="{Binding Path=P2000, Mode=OneWay}" Grid.Column="2" Name="txt_Twenties" Background="{DynamicResource TextBoxDIsableGradient}" BorderBrush="{DynamicResource TextBoxOutline}"  IsReadOnly="True" Margin="1,6,1,0" Style="{DynamicResource TextBoxStyle1}" TextAlignment="Right" TextWrapping="Wrap" Height="25" VerticalAlignment="Top" Width="100" PreviewKeyDown="EnterClicked" KeyboardNavigation.TabIndex="5"/>
