        <DataTemplate x:Uid="DataTemplate_2" x:Key="GMUtoMachineTemplate">
            <Grid x:Uid="Grid_2" MinWidth="10" HorizontalAlignment="Stretch">
                <Border x:Uid="Border_2" BorderBrush="Black" Margin="-7,-2,-5.5,-2" BorderThickness="0,0,0.5,0" VerticalAlignment="Stretch" />
                <TextBlock x:Uid="GMUtoMachine" x:Name="GMUtoMachine" Text="{Binding Path=GMUtoMachine}" HorizontalAlignment="Center" Margin="0,0,0,0" />
            </Grid>
            <DataTemplate.Triggers>
                <DataTrigger x:Uid="DataTrigger_4" Binding="{Binding GMUtoMachine}" Value="ERROR">
                    <Setter x:Uid="Setter_4" TargetName="GMUtoMachine" Property="Foreground" Value="Red" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_5" Binding="{Binding GMUtoMachine}" Value="OK">
                    <Setter x:Uid="Setter_5" TargetName="GMUtoMachine" Property="Foreground" Value="Green" />
                </DataTrigger>
                <DataTrigger x:Uid="DataTrigger_6" Binding="{Binding GMUtoMachine}" Value="UNKNOWN">
                    <Setter x:Uid="Setter_6" TargetName="GMUtoMachine" Property="Foreground" Value="Red" />
                </DataTrigger>
            </DataTemplate.Triggers>
        </DataTemplate>

      

