<?xml version="1.0"?>
<configuration>
    <appSettings>
      <add key="LogPath" value="C:\logs\ExchangeDialupServiceLog.txt"/>
      <add key="LogFolder" value="C:\logs\"/>
      <add key="LogMode" value="TXT"/>
      <add key="LogLevel" value="1"/>
      <add key="TimerIntervalinSecs" value="1"/>
      <add key="PerItemProcessMilliseconds" value="10"/>
      <add key="BGSAdminWSURL" value="http://[ENTERPRISE WEBSERVER]/BGSWSAdmin/BGSWSAdmin.asmx"/>
      <add key="RegistryPath" value="Cashmaster"/>
      <add key="StopSendingAfterNTimes" value="10"/>
      <add key="ApplicationName" value="ExchangeDialupService"/>
      <add key="Port" value="60000"/>
      <add key="Server" value="Exchange"/>
      <add key="ServerIP" value="10.2.100.85"/>
      <add key="CommandTimeOut" value="300"/>
      <add key="LimitDetailsCount" value="200"/>
      <!-- Performance Settings added by A.Vinod Kumar on 1:51 PM 08/06/12-->
      <add key="LimitThreadCount" value="True"/>
      <add key="ThreadCount" value="60"/>
      <add key="EmptyRecordCountCheck" value="30"/>

      <add key="EventTransmit_RetryRepeat" value="1"/>
      <add key="EventTransmit_RecoverOnError" value="1"/>
      <add key="MaxTransmissionRetry" value="1"/>
      <add key="IsTransmitterEnabled" value="1"/>    
      <add key="STMServerIP" value="http://10.11.144.136:9078/XMLDRService"/>
      <add key="STMServerPort" value="5555"/>
      <add key="EventTrasnmitterSrcQ" value=".\private$\STMQueue"/>
      <!--Setting Modes APP,DB,REG-->
      <add key="EventTrasnmitter_SettingsMode" value="DB"/>
      <add key="AppRelauncher_Enable" value="1"/>
      <add key="DeleteMessageOnParseErr" value="0"/>
      <!--DEBUG -> INFO-> WARNING -> ERROR (DEBUG Level includes all)-->
      <add key="AppLogLevel" value="DEBUG"/>
      <add key="STMAllowedMessages" value="10,96,97,80,79,56,71,72,176,4,207"/>
      

      <!--Setting STM Export-->
      <add key="STMExport_MaximumRetry" value="20"/>
      <add key="STMExport_DBHitInterval" value="2000"/>
      <add key="STMExport_EventSendInterval" value="50"/>
      
      <!--SiteLicensing Remote Server-->
      <add key="RemotingServer" value="SiteLicensingServer.Rem"/>
      <add key="RemotingServerPort" value="9095"/>
      
      <!--Value takes the No. of seconds-->
      <add key="RemotingServerInterval" value="5"/>

      <!-- CDO Central Server Settings -->
      <add key="CDOCentralServerPortNo" value="9099"/>
      <add key="LogIncomingMessages" value="False"/>
      <add key="LogOutgoingMessages" value="False"/>
      <add key="SetSlotStatusWaitTimeout" value="5000"/>
      <add key="SetFloorPositionWaitTimeout" value="5000"/>
      <add key="SetServiceStatusWaitTimeout" value="5000"/>
      <add key="LogClients" value="False"/>
  <!--In Milliseconds-->
      <add key="VaultMessageDBinterval" value="10000"/>
      <add key="SkipVaultErrorMessage" value="True"/>
      <add key="SetSiteLicensingWaitTimeout" value="60000"/>
         <!--Settings for the Immediate Logging
      ImmediateLogging false denotes loggs are buffered and written after buffer size had reached-->
    <add key="ImmediateLogging" value="false"/>
    <!--Default Value : 8096 bytes , Value in bytes -->
    <add key="LogBufferSize" value="8096"/>
    <add key="LogNoIdleTimer" value="False"/>
    <!--Default Value : 1 Minute , Value in Milli Seconds -->
    <add key="LogIdleTimeInterval" value="60000"/>
      
      
    
    </appSettings>
    
  </configuration>
